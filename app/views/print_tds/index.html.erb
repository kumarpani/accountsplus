<% if !params[:start_date].to_s.empty? && !params[:end_date].to_s.empty? %>
    <b> TDS between :
      <%= display_verbose_date(params[:start_date]) %>  &nbsp;&nbsp;|&nbsp;&nbsp;
      <%= display_verbose_date(params[:end_date]) %>
    </b>
<% end %>

<table class="printServiceTaxesTable table-bordered table-striped">
  <thead>
  <tr>
    <th><strong>Sl No</strong></th>
    <th><strong>Client Name</strong></th>
    <th><strong>Paid On</strong></th>
    <th><strong>Amount</strong></th>
  </tr>
  </thead>

  <tbody>
  <% @tds.each_with_index do |t, index| %>
      <tr>
        <td class="alignCenter" width=40px;><%= index + 1 %></td>
        <td width=300px;><%= get_company_name_by_client_id(t[:client_id]) %></td>
        <td class="alignCenter" width=100px;><%= display_date(t[:paid_on]) %></td>
        <td class="alignRight" width=100px><%= t[:amount] %></td>
      </tr>
  <% end %>
  <tr>
    <td></td>
    <td></td>
    <td><b> Total: </b></td>
    <td class="alignRight"><b><%= @tds.sum(&:amount) %></b></td>
  </tr>

  </tbody>
</table>